// Place your key bindings in this file to overwrite the defaults
[
{ "key": "alt+f12",        "command": "editor.action.previewDeclaration",
                              "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor" },
{ "key": "ctrl+m",         "command": "projectManager.listProjects" },
{ "key": "ctrl+shift+m",   "command": "projectManager.listProjectsNewWindow" },
{ "key": "ctrl+shift+-",    "command": "workbench.action.zoomOut" },
{ "key": "ctrl+-",          "command": "workbench.action.navigateBack" }
]
